    from flask import Flask, render_template, jsonify, redirect

    app = Flask(__name__)

    app.config['DEBUG'] = True
    app.config['SECRET_KEY'] = "dev-key"

    @app.route('/')
    def index():
        return render_template('greet.html')

    @app.route('/greet')
    def greet():
        return jsonify({"greeting":"hello"})

    if __name__ == '__main__':
        app.run()
